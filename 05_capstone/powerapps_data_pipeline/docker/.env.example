# Environment variables for Docker Compose
# Copy to .env and modify for your environment

# Pipeline Configuration
PIPELINE_ENV=development  # development, staging, production
DATA_DIR=/app/data

# PostgreSQL Configuration (for production)
POSTGRES_DB=powerapps_warehouse
POSTGRES_USER=pipeline
POSTGRES_PASSWORD=SecurePassword123!  # CHANGE THIS IN PRODUCTION

# pgAdmin Configuration (development only)
PGADMIN_EMAIL=admin@powerapps.local
PGADMIN_PASSWORD=admin  # CHANGE THIS

# Jupyter Configuration (development only)
JUPYTER_TOKEN=devtoken  # CHANGE THIS

# Timezone
TZ=America/Chicago
